    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.GET_TASKS"/>

    <application
        android:name=".Util.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        tools:replace="android:icon,android:name,android:theme ">
        <meta-data
            android:name="AA_DB_NAME"
            android:value="jchat.db"/>
        <meta-data
            android:name="AA_MODELS"
            android:value="com.example.chen.wsscapp.Bean.UserEntry, com.example.chen.wsscapp.Bean.FriendEntry,
            com.example.chen.wsscapp.Bean.FriendRecommendEntry, com.example.chen.wsscapp.Bean.ConversationEntry"/>
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="4"/>
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MainActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.BecomeShopActivity" />
        <activity android:name=".activity.SetUserInfoActivity" />
        <activity android:name=".activity.SetNicknameActivity" />
        <activity android:name=".activity.SetSexActivity" />
        <activity android:name=".activity.SetAddrActivity" />
        <activity android:name=".activity.SetBirthActivity" />
        <activity android:name=".activity.SetTouxiangActivity" />
        <activity android:name=".activity.BecomephotoActivity" />
        <activity android:name=".activity.ShowshopInfoActivity" />
        <activity android:name=".activity.KeySearchShopActivity" />
        <activity android:name=".activity.WeiTaoDetail" />
        <activity android:name=".activity.SetPhoneActivity" />
        <activity android:name=".activity.SetNewphoneActivity" />

        <receiver
            android:name=".MyJGReceiver"
            android:enabled="true">
            <intent-filter>
                <action android:name="cn.jpush.android.intent.RECEIVE_MESSAGE" />

                <category android:name="com.example.chen.wsscapp" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".MyOldJGReceuver"
            android:enabled="true">
            <intent-filter>

                <!-- Required 用户注册SDK的intent -->
                <action android:name="cn.jpush.android.intent.REGISTRATION" />
                <!-- Required 用户接收SDK消息的intent -->
                <action android:name="cn.jpush.android.intent.MESSAGE_RECEIVED" />
                <!-- Required 用户接收SDK通知栏信息的intent -->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_RECEIVED" />
                <!-- Required 用户打开自定义通知栏的intent -->
                <action android:name="cn.jpush.android.intent.NOTIFICATION_OPENED" />
                <!-- 接收网络变化 连接/断开 since 1.6.3 -->
                <action android:name="cn.jpush.android.intent.CONNECTION" />
                <category android:name="com.example.chen.wsscapp" />
            </intent-filter>
        </receiver>
        <provider
            android:authorities="com.example.chen.wsscapp.DataProvider"
            android:name="cn.jpush.android.service.DataProvider"
            android:exported="true"
            tools:replace="android:exported"
            />

        <activity
            android:name=".activity.PushInfo"
            android:label="@string/title_activity_push_info"
            android:theme="@style/Theme.AppCompat.NoActionBar"></activity>
        <activity android:name=".activity.BrowserViewPagerActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:screenOrientation="portrait"
            android:name=".activity.ChatActivity"
            android:theme="@style/BaseThemes"/>
        <activity
            android:screenOrientation="portrait"
            android:name=".activity.FeedbackActivity"
            android:theme="@style/BaseThemes"/>

        <activity
            android:screenOrientation="portrait"
            android:name=".activity.SendFileActivity"
            android:launchMode="singleInstance"
            android:theme="@style/AppBaseTheme"/>

        <activity android:name=".activity.PlayVideoActivity"
            android:screenOrientation="portrait"/>


        <activity
            android:screenOrientation="portrait"
            android:name=".activity.DownLoadActivity"
            android:theme="@style/BaseThemes"/>
        <activity android:name=".activity.ShopManagerActivity"/>
        <activity android:name=".activity.XcMenuActivity"/>
        <activity android:name=".activity.ShopDynamicActivity"/>
        <activity android:name=".activity.OrderActivity"/>
        <activity android:name=".activity.SendMsgActivity"/>
        <activity android:name=".activity.ChangeGetTypeActivity"/>
        <activity android:name=".activity.ChangeShopListActivity"/>
        <activity android:name=".activity.ChangeInfoActivity"/>
        <activity android:name=".activity.ChangeColSizeActivity"/>
        <activity android:name=".activity.ChangePhotoActivity"/>
        <activity android:name=".activity.ShopManagerOrderActivity"/>
        <activity android:name=".activity.ShowSeeaddrActivity"/>

    </application>

</manifest>